{
  "name": "maki-web-react",
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/maki-math/maki-web-hexo"
  },
  "scripts": {
    "dev": "vite",
    "dev-w-local-server": "vite --mode development-local-server",
    "build": "vite build",
    "serve": "vite preview",
    "gen-api-localhost": "npx swagger-typescript-api --type-suffix Model -p http://127.0.0.1:8000/schema/ -o ./src/generated-api --clean-output --axios",
    "gen-api": "npx swagger-typescript-api --type-suffix Model -p http://api.maki-math.com/schema/ -o ./src/generated-api --clean-output --axios"
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "ahooks": "^3.3.0",
    "antd": "^4.16.13",
    "axios": "^0.26.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5",
    "remarkable": "^2.0.1",
    "vditor": "^3.8.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-react-refresh": "^1.1.3",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "4.3.8",
    "less": "^4.1.2",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "swagger-typescript-api": "^9.3.1",
    "textlint": "^12.0.2",
    "textlint-rule-ja-space-between-half-and-full-width": "^2.2.0",
    "typescript": "^4.6.0-dev.20211114",
    "vite": "^2.0.0-beta.50"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": "textlint --fix"
  }
}

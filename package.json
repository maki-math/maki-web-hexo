{
  "name": "maki-web-react",
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/maki-math/maki-web-hexo"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000",
    "dev-w-local-server": "vite --port 3000 --mode development-local-server",
    "build": "vite build",
    "serve": "vite preview",
    "tc": "tsc --noEmit",
    "lint": "eslint --fix src",
    "lint-error-only": "eslint --fix --quiet src",
    "gen-api-localhost": "npx swagger-typescript-api --type-suffix Model -p http://127.0.0.1:8000/schema/ -o ./src/generated-api --clean-output --axios",
    "gen-api": "npx swagger-typescript-api --type-suffix Model -p http://api.maki-math.com/schema/ -o ./src/generated-api --clean-output --axios"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.5.6",
    "@ant-design/pro-layout": "^7.4.0",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "ahooks": "^3.3.0",
    "ali-oss": "^6.17.1",
    "antd": "5.1.7",
    "axios": "^0.26.1",
    "constate": "^3.3.2",
    "dayjs": "^1.11.7",
    "minimatch": ">=3.0.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-ga4": "^1.4.1",
    "react-github-btn": "^1.4.0",
    "react-router-dom": "^5",
    "vditor": "^3.8.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/ali-oss": "^6.16.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-react-refresh": "^1.1.3",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "4.3.8",
    "less": "^4.1.2",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "swagger-typescript-api": "^9.3.1",
    "textlint": "^12.0.2",
    "textlint-rule-ja-space-between-half-and-full-width": "^2.2.0",
    "typescript": "^4.8.0-dev.20220723",
    "vite": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": "textlint --fix"
  }
}
